{
  "processesNum"    : 20000,
  "processesLambda" : 1,
  "processorsNum"   : 2,
  "processorLambda" : 1,
  "logging": {
    "version": 1,
    "loggers": {
      "mdes.simulator": {
        "level": "INFO",
        "handlers": ["console"]
      },
      "mdes.cmd": {
        "level": "INFO",
        "handlers": ["console"]
      },
      "mdes.event": {
        "level": "INFO",
        "handlers": ["console"]
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "default"
      }
    },
    "formatters": {
      "default": {
        "format": "%(asctime)s %(levelname)-6s %(name)s %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
    }
  }
}
